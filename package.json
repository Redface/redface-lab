{
  "name": "redface-lab",
  "version": "1.0.0",
  "description": "jazzyredfaces' lab session",
  "main": "index.js",
  "scripts": {
    "start": "npm run server",
    "server": "npm run server:local",
    "server:local": "NODE_ENV=local webpack-dev-server --config client-config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:local:was": "NODE_ENV=local-server webpack-dev-server --config client-config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:dev": "NODE_ENV=dev webpack-dev-server --config client-config/webpack.dev.js --progress --profile --watch --content-base src/",
    "clean": "rm -rf node_modules ./client-config/coverage dist",
    "clean:dist": "rm -rf ./dist",
    "clean:install": "npm run clean && npm install",
    "build": "npm run build:local",
    "build:local": "NODE_ENV=local node ./node_modules/.bin/webpack --config client-config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached --hide-modules",
    "build:local:was": "NODE_ENV=local-server node ./node_modules/.bin/webpack --config client-config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached --hide-modules && npm run dist-to-was",
    "build:prod": "NODE_ENV=prod node ./node_modules/.bin/webpack --config client-config/webpack.prod.js --progress --profile --colors --display-error-details --display-cached --hide-modules",
    "build:aot": "NODE_ENV=prod node ./node_modules/.bin/webpack --config client-config/webpack.aot.js --progress --profile --colors --display-error-details --display-cached --hide-modules",
    "webdriver-manager": "./node_modules/.bin/webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "pree2e": "npm run webdriver:update -- --standalone"
  },
  "author": "jazzyredface",
  "license": "ISC",
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "dependencies": {
    "@angular/animations": "^4.3.0-beta.1",
    "@angular/common": "^4.3.0-beta.1",
    "@angular/compiler": "^4.3.0-beta.1",
    "@angular/compiler-cli": "^4.3.0-beta.1",
    "@angular/core": "^4.3.0-beta.1",
    "@angular/forms": "^4.3.0-beta.1",
    "@angular/http": "^4.3.0-beta.1",
    "@angular/platform-browser": "^4.3.0-beta.1",
    "@angular/platform-browser-dynamic": "^4.3.0-beta.1",
    "@angular/platform-server": "^4.3.0-beta.1",
    "@angular/router": "^4.3.0-beta.1",
    "@angular/upgrade": "^4.3.0-beta.1",
    "angular-in-memory-web-api": "^0.3.2",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.1",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.4.2",
    "@types/core-js": "^0.9.36",
    "@types/node": "^7.0.32",
    "angular-router-loader": "^0.6.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "codelyzer": "^3.1.1",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^2.2.0",
    "ts-node": "^3.1.0",
    "tslint": "^4.3.1",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.3.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^4.1.0"
  }
}
